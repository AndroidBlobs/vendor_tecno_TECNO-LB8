<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        Mozambique
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="112" Category="0" Condition="1" Name="Emergency Number" />
    <EccEntry Ecc="119" Category="0" Condition="2" Name="Police" />
    <!-- CMT: modify chenlei LAQGHSJS-1149 20180327 start -->
    <EccEntry Ecc="198" Category="0" Condition="2" Name="Mozambique Fire" />
    <!-- CMT: modify chenlei LAQGHSJS-1149 20180327 end -->
</EccTable>
